{
  "private": true,
  "scripts": {
    "dev": "run-s clean && node server.js",
    "build": "run-s clean && next build",
    "lint": "next lint",
    "export:next": "next export",
    "export:posts-assets": "node scripts/copy-assets.js",
    "export": "run-s build export:next export:posts-assets",
    "format": "prettier --config .prettierrc '**/*.+(ts|tsx)' --write",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "coveralls": "npm run test:coverage && coveralls < coverage/lcov.info",
    "clean": "rm -rf .tmp"
  },
  "dependencies": {
    "@pondorasti/remark-img-links": "^1.0.8",
    "classnames": "2.3.1",
    "date-fns": "2.28.0",
    "glob": "^8.0.1",
    "gray-matter": "4.0.3",
    "lodash": "^4.14.182",
    "next": "^12.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remark": "14.0.2",
    "remark-html": "15.0.1",
    "unist-util-visit": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "autoprefixer": "^10.4.2",
    "coveralls": "^3.1.1",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "express": "^4.17.3",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "prettier": "^2.6.2",
    "recursive-copy": "^2.0.14",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16"
  }
